<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <ul>
        <%  booksInfo.forEach(book =>{ %>
        <li>
            <img src=<%= book.thumbnail %> />
        </li>
        <li>
            <%= book.title %>
        </li>
        <li>
            <%= book.authorName %>
        </li>
        <li>
            <%= book.description %>
        </li>
        <%  }) %>
    </ul>
</body>

</html>